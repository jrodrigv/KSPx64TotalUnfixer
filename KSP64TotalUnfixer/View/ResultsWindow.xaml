<Window x:Class="KSPx64TotalUnfixer.UI.View.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ignore="http://www.ignore.com"
        mc:Ignorable="d ignore"
        Height="600"
        Width="600"
        Title="Results"  
        DataContext="{Binding Results, Source={StaticResource Locator}}" Icon="../KSPx64TotalUnfixer_ico.ico">
    <Grid Margin ="20"> 
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <TextBlock  Text ="{Binding ResultsOutput}" FontSize="8.0" ></TextBlock>
        </ScrollViewer>
       
    </Grid>
</Window>
